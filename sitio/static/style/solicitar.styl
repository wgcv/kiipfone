.solicitar
	text-align center
	height: 100vh;
.marcas
	width 75%
	margin 0 auto 5em auto
	img
		display block
		margin 0.5 em
		width: 150px;
		height: 150px;
.inicial a
	display: inline-block;
	color #000
	text-decoration none
	&:hover
		transform: scale(0.9);
.reparacion
	padding: .9em 0;
	border-style: solid;
	border-color: #0388a6;
	margin: 1em;
	width: 200px;
	color: #000!important;
	border-radius: 22px;